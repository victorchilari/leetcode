{
	"name": "leetcode",
	"version": "0.2.0",
	"private": true,
	"scripts": {
		"server": "node server/index",
		"start": "concurrently \"docusaurus start\" \"npm run server\"",
		"deployOnGitHub": "cmd /C \"set \"GIT_USER=victorchilari\" && npm run deploy\"",
		"prepare-packages": "npm install && npm run prepare-husky",
		"prepare-husky": "husky install",
		"prettify-docs": "prettier --write docs/**/*",
		"docusaurus": "docusaurus",
		"build": "docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids"
	},
	"lint-staged": {
		"docs/*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"@docusaurus/core": "2.0.0-beta.3",
		"@docusaurus/preset-classic": "2.0.0-beta.3",
		"@mdx-js/react": "^1.6.21",
		"@svgr/webpack": "^5.5.0",
		"clsx": "^1.1.1",
		"dotenv": "^10.0.0",
		"file-loader": "^6.2.0",
		"prism-react-renderer": "^1.2.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"url-loader": "^4.1.1"
	},
	"devDependencies": {
		"concurrently": "^6.2.0",
		"express": "^4.17.1",
		"husky": "^7.0.1",
		"lint-staged": "^11.1.1",
		"prettier": "^2.3.2"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
